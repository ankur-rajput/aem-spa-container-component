<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/aem-spa-container-component/settings/wcm/templates/spa-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aem-spa-container-component/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aem-spa-container-component/components/experiencefragment"
                                fragmentVariationPath="/content/experience-fragments/aem-spa-container-component/us/en/site/header/master"/>
                

            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                editable="{Boolean}true">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
            </responsivegrid>

            <experiencefragment-footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aem-spa-container-component/components/experiencefragment"
                        fragmentVariationPath="/content/experience-fragments/aem-spa-container-component/us/en/site/footer/master"/>
            

        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <small
                  jcr:primaryType="nt:unstructured"
                  title="Small"
                  width="{Long}899"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
